<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net462</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Joel Christner</Authors>
    <Description>Emulated Amazon Web Services (AWS) Simple Storage Service (S3) server-side interface.</Description>
    <Copyright>(c)2019 Joel Christner</Copyright>
    <PackageReleaseNotes>Initial release</PackageReleaseNotes>
    <PackageTags>aws s3 storage rest interface server</PackageTags>
    <PackageLicenseUrl>https://github.com/jchristn/S3ServerInterface/blob/master/LICENSE.txt</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/jchristn/S3ServerInterface</PackageProjectUrl>
    <RepositoryUrl>https://github.com/jchristn/S3ServerInterface</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Watson" Version="1.6.0" />
  </ItemGroup>

</Project>
