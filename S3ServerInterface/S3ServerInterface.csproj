<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net462</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Joel Christner</Authors>
    <Description>Emulated Amazon Web Services (AWS) Simple Storage Service (S3) server-side interface.</Description>
    <Copyright>(c)2019 Joel Christner</Copyright>
    <PackageReleaseNotes>Various fixes, removed class deserialization (must be handled by the application implementing the callbacks)</PackageReleaseNotes>
    <PackageTags>aws s3 storage rest interface server</PackageTags>
    <PackageLicenseUrl>https://github.com/jchristn/S3ServerInterface/blob/master/LICENSE.txt</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/jchristn/S3ServerInterface</PackageProjectUrl>
    <RepositoryUrl>https://github.com/jchristn/S3ServerInterface</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <Version>1.4.15</Version>
    <PackageIconUrl>https://raw.githubusercontent.com/jchristn/S3ServerInterface/master/assets/logo.ico</PackageIconUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.S3" Version="3.3.102.19" />
    <PackageReference Include="ChunkDecoder" Version="1.0.2" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
    <PackageReference Include="Watson" Version="2.1.1" />
  </ItemGroup>

</Project>
